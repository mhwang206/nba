<div class="main">

	<div class="logout">_one
	<%= button_to "Log out", auths_path, method: :delete %>
	</div>
	<div class="create_poll">
	<%= link_to "Create a poll", new_poll_path%>
	</div>

	<h1>Polls</h1>
	<div class ="welcome_msg">
	Hello, <%= current_user ? current_user.username : "(not logged in)" %> 
	</div>


	<% @polls.each do |f| %>
		<div class="poll">
			<%= image_tag(f.image_url, size: '310x178') %>
			<br>
			<%= f.question %>
			<br>
			<%= f.choice_one %>
			<br>
			<%= f.choice_two %>
		</div>
	<% end %>

	
</div>

		